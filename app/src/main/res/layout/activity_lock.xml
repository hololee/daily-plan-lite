<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">


    <TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp"
        android:gravity="center"
        android:text="@string/insertPass"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="22dp" />

    <RelativeLayout
        android:layout_width="240dp"
        android:layout_height="40dp"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/text"
        android:layout_centerHorizontal="true">


        <ImageView
            android:id="@+id/passclip1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:src="@drawable/passward_false" />

        <ImageView
            android:id="@+id/passclip2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_toRightOf="@+id/passclip1"
            android:src="@drawable/passward_false" />

        <ImageView
            android:id="@+id/passclip3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_toRightOf="@+id/passclip2"
            android:src="@drawable/passward_false" />

        <ImageView
            android:id="@+id/passclip4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_toRightOf="@+id/passclip3"
            android:src="@drawable/passward_false" />


    </RelativeLayout>

    <ImageButton

        android:id="@+id/imageButton3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_0" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/imageButton3"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25dp"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_8" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/imageButton4"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25dp"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_5" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/imageButton5"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25dp"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_2" />


    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_marginRight="60dp"
        android:layout_toStartOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_7" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_marginRight="60dp"
        android:layout_toStartOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_4" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_marginRight="60dp"
        android:layout_toStartOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_1" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:paddingRight="8dp"
        android:paddingLeft="8dp"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_marginLeft="60dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_delete" />


    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_marginLeft="60dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_9" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_marginLeft="60dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_6" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_marginLeft="60dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:background="@null"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:scaleType="centerInside"
        android:src="@drawable/button_3" />

    <TextView
        android:textColor="#c5c5c5"
        android:layout_marginBottom="2dp"
        android:textSize="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:gravity="center"
        android:text="@string/hint"
        android:id="@+id/textView"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>